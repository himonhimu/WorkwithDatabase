<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="bd.himu.himon.workwithdatabase">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainFunction">
            <intent-filter>
                <action android:name="bd.himu.himon.workwithdatabase.MainFunction" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".BcsClass.BcsQuestionList">
            <intent-filter>
                <action android:name="bd.himu.himon.workwithdatabase.BcsQuestionList" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SubjectiveClass.SujectiveAll"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.Bangla" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SubjectiveClass.SubjectiveList">
            <intent-filter>
                <action android:name="android.intent.action.Subjectivelist" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BcsClass.BcsAll"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.Bcs38" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>